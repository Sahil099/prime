#include<stdio.h>
int main()
{
 int n,i,a,b=0,c;

 scanf("%d",&n);

 if(n>1)
 {
  c=1;
  i=2; 
 
  while(i<=n)
  {
   a=2; 
   while(a<i)
   {
    b=i%a;
    if(b==0)
    {
     break;
    }
    a++;
   }
   
  if (b!=0)
  {
   c++;
  }
  i++;
 }
}

 else
 {
  c=0;
 }
 
printf("%d",c);

return 0;
}
